{
  "name": "@rust-commerce/client",
  "version": "0.1.0",
  "description": "TypeScript client for Rust Commerce microservices",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "generate": "npm run generate:proto && npm run generate:nats-config",
    "generate:proto": "protoc --plugin=./node_modules/.bin/protoc-gen-ts_proto --ts_proto_out=./generated --ts_proto_opt=esModuleInterop=true,outputEncodeMethods=true,outputJsonMethods=true,outputClientImpl=false,snakeToCamel=true --proto_path=../catalog/proto --proto_path=../shared-proto/proto ../catalog/proto/*.proto ../shared-proto/proto/**/*.proto",
    "generate:nats-config": "tsx scripts/generate-nats-config.ts",
    "build": "npm run generate && tsc",
    "dev": "tsc --watch",
    "test": "echo \"Tests not yet implemented\" && exit 0",
    "lint": "echo \"Linting not yet configured\" && exit 0",
    "format": "prettier --write \"src/**/*.{ts,tsx,json,md}\"",
    "example": "tsx examples/basic-usage.ts"
  },
  "dependencies": {
    "nats": "^2.28.0",
    "protobufjs": "^7.3.2"
  },
  "devDependencies": {
    "@types/jest": "^29.5.12",
    "@types/node": "^20.14.10",
    "@typescript-eslint/eslint-plugin": "^7.16.0",
    "@typescript-eslint/parser": "^7.16.0",
    "eslint": "^8.57.0",
    "jest": "^29.7.0",
    "prettier": "^3.3.2",
    "ts-jest": "^29.1.5",
    "ts-proto": "^1.181.2",
    "tsx": "^4.16.2",
    "typescript": "^5.5.3"
  },
  "files": [
    "dist",
    "generated"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/your-org/rust-commerce.git"
  },
  "keywords": [
    "rust",
    "commerce",
    "microservices",
    "nats",
    "protobuf"
  ],
  "author": "",
  "license": "MIT"
}